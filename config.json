{
  "flashml": {
    "context": "local[*]",
    "rootDirectory": "/user/cloudera"
  },
  "model": {
    "id": "MODEL_id1"
  },
  "project": {
    "id": "multiIntent_SVM_titanic",
    "logLevel": "ERROR",
    "data": {
      "location": {
        "source": "hive://FLASHML.TITANIC_SURVIVAL_DATA",
        "format": "json",
        "temp_table_prefix": "flashml_table_",
        "queries": [
        ]
      },
      "schema": {
        "file": "support_files/schema.txt"
      }
    }
  },
  "apollo": {
    "api": {
      "url": "http://localhost:8383/jobs/job1/status"
    }
  },
  "job": {
    "id": "job1"
  },
  "vertica": {
    "host": {
      "url": "jdbc:vertica://analytics04.pxoe-app.va1.247-inc.net:5433/vertica247"
    },
    "jdbc": {
      "driver": "com.vertica.jdbc.Driver"
    },
    "user": {
      "name": "user_name",
      "password": "password"
    }
  },
  "experiment": {
    "variables": {
      "scope": "noPage",
      "numerical": ["age"],
      "categorical": ["pclass","sex"],
      "text": [
        "cabin"
      ]
    },
    "cv": {
      "folds": 1,
      "predictSavepoint": false
    },
    "modelingMethod": [
      "multi_intent"
    ],
    "type": "model",
    "algorithm": {
      "type": "svm",
      "build": {
        "type": "ovr"
      },
      "svm": {
        "plattScalingEnabled": true,
        "regparam": 0.1,
        "maxiter": 20,
        "standardization": true
      }
    },
    "retrainId": "original",
    "pageLevel": {
      "numberOfPages": 1
    },
    "multiIntent": {
      "topIntentsColumn": "top_intents",
      "maxTopIntents": 10
    },
    "primaryKey": [
      "home_dest"
    ],
    "responseVariable": "survived",
    "pageVariable": "",
    "dateVariable": "",
    "randomVariable": "",
    "data": {
      "positivePercent": 10
    },
    "randomNumberGeneratorVariable": "home_dest",
    "uplift": {
      "treatmentVariable": ""
    },
    "cumulativeSessionTime": "",
    "additionalVariables": [],
    "customFilter": "",
    "postPredictFilter": "",
    "sample": {
      "type": "random",
      "split": [
        70,30
      ],
      "condition": [],
      "stratified": {
        "minimumClassSupport": 10,
        "otherClassValue": "other-other",
        "minimumClassSupportRequired": false
      }
    },
    "preprocessing": {
      "scope": "noPage",
      "steps": [
        {
          "inputVariable": "pclass",
          "transformations": [
            {
              "type": "nullcheck",
              "parameter": "0"
            }
          ]
        },
        {
          "inputVariable": "age",
          "transformations": [
            {
              "type": "nullcheck",
              "parameter": 0
            }
          ]
        },
        {
          "inputVariable": "cabin",
          "outputVariable": "cabin1",
          "transformations": [
            {
              "type": "nullcheck",
              "parameter": "default"
            },
            {
              "type": "case_normalization"
            },
            {
              "type": "tokenizer",
              "parameter": "[\\s+]"
            }
          ]
        }
      ]
    },
    "featuregeneration": {
      "scope": "noPage",
      "binning": [],
      "grams": [
        {
          "inputVariable": "cabin1",
          "outputVariable": "cabin2",
          "transformations": [
            {
              "ngram": [

              ]
            }
          ]
        }
      ]
    },
    "vectorization": {
      "text": {
        "scope": "noPage",
        "steps": [
          {
            "inputVariable": "cabin2",
            "method":"count_vectorizer",
            "slots": 1500
          }
        ]
      },
      "categorical": {
        "method": "hashingtf",
        "slots": 1000
      }
    },
    "publish": {
      "format": "mleap",
      "thresholds": [
      ],
      "topList": [
      ],
      "online": {
        "pages": [
        ],
        "top": [
        ]
      }
    },
    "qa": {
      "format": "json",
      "dataPoints": 50
    }
  },
  "hdfs": {
    "nameNode": {
      "uri": "hdfs://localhost:9000"
    }
  },
  "hive": {
    "thrift": {
      "url": "thrift://localhost:9083"
    }
  },
  "pipeline": {
    "steps": [
      "datareader",
      "sampling",
      "preprocessing",
      "featuregeneration",
      "vectorization",
      "modelling",
      "scoring",
      "standardmetrics",
      "publish"
    ]
  },
  "savePointing": {
    "required": true
  }
}